<?xml version="1.0" encoding="utf-8"?>
<mx:Application xmlns:mx="http://www.adobe.com/2006/mxml" xmlns="*"
    layout="absolute" styleName="plain"
    verticalScrollPolicy="off" horizontalScrollPolicy="off"
    width="100%" height="100%"
    preloader="com.threerings.msoy.client.Preloader"
    applicationComplete="viewStack.addChild(new RemixControls(this, viewStack));">
<mx:Style source="msoy.css"/>
<mx:Style source="com/threerings/msoy/applets/applets.css"/>
<mx:Style source="com/threerings/msoy/applets/remixer/remixer.css"/>
<mx:Style source="com/threerings/msoy/applets/image/image.css"/>
<mx:Style>
   LinkButton {
      color: #000000;
   }
</mx:Style>
<mx:Script>
    <![CDATA[
        import com.threerings.msoy.applets.remixer.RemixControls;
    ]]>
</mx:Script>
<mx:ViewStack id="viewStack"/>
</mx:Application>
