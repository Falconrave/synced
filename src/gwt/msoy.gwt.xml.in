<module>
  <inherits name="com.google.gwt.user.User"/>
  <inherits name="com.google.gwt.http.HTTP"/>
  <inherits name="com.google.gwt.i18n.I18N"/>
  <inherits name="com.google.gwt.xml.XML"/>

  <source path="org/gwtwidgets/client"/>
  <source path="com/kisgergely/gwt/canvas/client"/>
  <source path="com/threerings/io"/>
  <source path="com/threerings/gwt"/>
  <source path="com/threerings/util"/>
  <source path="com/threerings/presents/dobj"/>
  <source path="com/threerings/msoy/web/data"/>
  <source path="com/threerings/msoy/web/client"/>
  <source path="com/threerings/msoy/data/all"/>
  <source path="com/threerings/msoy/admin/gwt"/>
  <source path="com/threerings/msoy/fora/gwt"/>
  <source path="com/threerings/msoy/game/data/all"/>
  <source path="com/threerings/msoy/game/gwt"/>
  <source path="com/threerings/msoy/group/data/all"/>
  <source path="com/threerings/msoy/group/gwt"/>
  <source path="com/threerings/msoy/item/data/all"/>
  <source path="com/threerings/msoy/item/gwt"/>
  <source path="com/threerings/msoy/landing/gwt"/>
  <source path="com/threerings/msoy/mail/gwt"/>
  <source path="com/threerings/msoy/feed/gwt"/>
  <source path="com/threerings/msoy/person/gwt"/>
  <source path="com/threerings/msoy/profile/gwt"/>
  <source path="com/threerings/msoy/stuff/gwt"/>
  <source path="com/threerings/msoy/swiftly/data/all"/>
  <source path="com/threerings/msoy/swiftly/gwt"/>
  <source path="com/threerings/msoy/world/gwt"/>
  <source path="com/threerings/underwire/web/data"/>
  <source path="com/threerings/underwire/web/client"/>
  <source path="com/threerings/underwire/gwt/client"/>
  <source path="client"/>
  <public path="public"/>

  <entry-point class="client.Application"/>

  <replace-with class="client.ui.PopupImplMozillaHack">
    <when-type-is class="com.google.gwt.user.client.ui.impl.PopupImpl"/>
    <any>
      <when-property-is name="user.agent" value="gecko"/>
      <when-property-is name="user.agent" value="gecko1_8"/>
    </any>
  </replace-with>

  <replace-with class="com.threerings.gwt.ui.WidgetUtilImplMozilla">
    <when-type-is class="com.threerings.gwt.ui.WidgetUtilImpl"/>
    <any>
      <when-property-is name="user.agent" value="gecko"/>
      <when-property-is name="user.agent" value="gecko1_8"/>
    </any>
  </replace-with>

  <replace-with class="com.threerings.gwt.ui.WidgetUtilImplIE6">
    <when-type-is class="com.threerings.gwt.ui.WidgetUtilImpl"/>
    <any>
      <when-property-is name="user.agent" value="ie6"/>
    </any>
  </replace-with>
</module>
