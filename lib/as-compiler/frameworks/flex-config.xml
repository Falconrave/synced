<flex-config>
   <compiler>
      <!-- Turn on generatation of accessible swfs. -->
      <accessible>false</accessible>
      <!-- List of path elements that form the roots of ActionScript class hierarchies. -->
      <!-- not set -->
      <!--
      <source-path>
         <path-element>locale/{locale}</path-element>
         <path-element>string</path-element>
      </source-path>
      -->
      <!-- Run the AS3 compiler in a mode that detects legal but potentially incorrect -->
      <!-- code.                                                                       -->
      <show-coach-warnings>true</show-coach-warnings>
      <!-- Turn on generatation of debuggable swfs.  False by default for mxmlc, -->
      <!-- but true by default for compc. -->
      <!--
      <debug>true</debug>
      -->	  
      <!-- List of SWC files or directories to compile against but to omit from -->
      <!-- linking.                                                             -->
      <external-library-path>
         <path-element>libs/playerglobal.swc</path-element>
      </external-library-path>
      <!-- Turn on writing of generated/*.as files to disk. These files are generated by -->
      <!-- the compiler during mxml translation and are helpful with understanding and   -->
      <!-- debugging Flex applications.                                                  -->
      <keep-generated-actionscript>false</keep-generated-actionscript>
      <!-- not set -->
      <!--
      <include-libraries>
         <library>string</library>
      </include-libraries>
      -->
      <!-- List of SWC files or directories that contain SWC files. -->
      <library-path>
         <path-element>libs</path-element>
         <path-element>locale/{locale}</path-element>

      </library-path>
      <namespaces>
         <!-- Specify a URI to associate with a manifest of components for use as MXML -->
         <!-- elements.                                                                -->
         <namespace>
            <uri>http://www.adobe.com/2006/mxml</uri>
            <manifest>mxml-manifest.xml</manifest>
         </namespace>
      </namespaces>
      <!-- Enable post-link SWF optimization. -->
      <optimize>true</optimize>
      <!-- Turn on reporting of data binding warnings.  For example: Warning: Data binding -->
      <!-- will not be able to detect assignments to "foo".                                -->
      <show-binding-warnings>true</show-binding-warnings>
      <!-- Show warnings when dependencies cannot be determined at compile time.           -->
      <show-dependency-warnings>false</show-dependency-warnings>
      <!-- Show warnings when deprecated classes, variables and functions are used. -->
      <show-deprecation-warnings>true</show-deprecation-warnings>
      <!-- Run the AS3 compiler in strict error checking mode.
           strict must be 'false' when es3 is 'true'. -->
      <strict>true</strict>
      <!-- Run the compiler in AS3 compatibility mode. Either as3 or es3 should be true. -->
      <as3>true</as3>
      <!-- Run the compiler in ES3 compatibility mode -->
      <es3>false</es3>
      <!-- List of CSS or SWC files to apply as a theme. -->
      <!-- not set -->
      <!--
      <theme>
         <filename>string</filename>
         <filename>string</filename>
      </theme>
      -->
      <!-- Turns on the display of stack traces for uncaught runtime errors. -->
      <verbose-stacktraces>false</verbose-stacktraces>
      <!-- Defines the AS3 file encoding. -->
      <!-- not set -->
      <!--
      <actionscript-file-encoding></actionscript-file-encoding>
      -->
      <!-- Enables reporting of undeclared property references.  Referencing undefined     -->
      <!-- throws a 'ReferenceError' in AS3, rather than evaluating to undefined as it did -->
      <!-- in AS2.                                                                         -->
      <warn-undefined-reference>false</warn-undefined-reference>
      <!-- Enables reporting of undefined function calls.  Calling undefined functions       -->
      <!-- throws a 'ReferenceError' in AS3, rather than failing  silently as it did in AS2. -->
      <warn-undefined-function>true</warn-undefined-function>
      <!-- Enables reporting of incorrect number of arguments. -->
      <warn-wrong-number-of-args>true</warn-wrong-number-of-args>
      <!-- Enables reporting of asssignments of 'undefined' to variables of type Boolean, -->
      <!-- Number, uint, or int.                                                          -->
      <warn-assigning-undefined-to-non-object-type>true</warn-assigning-undefined-to-non-object-type>
      <!-- Enables reporting of missing type declarations. -->
      <warn-no-type-decl>true</warn-no-type-decl>
      <!-- Enables reporiting of incorrect comparision with 'undefined'.  In AS3,   -->
      <!-- variables of type Boolean, Number, uint, and int can never be undefined. -->
      <warn-bad-undefined-comparision>true</warn-bad-undefined-comparision>
      <!-- Enables reporting incorrect argument types for function calls. -->
      <warn-wrong-arg-type>true</warn-wrong-arg-type>
      <!-- Enables reporting when a variable has the same name as a function, which -->
      <!-- results in the variable being hidden.                                    -->
      <warn-var-hidden-by-function>true</warn-var-hidden-by-function>
      <!-- Enables reporting of multiple definitions of the same function. -->
      <warn-duplicate-func-def>true</warn-duplicate-func-def>
      <!-- Enables reporting when a function used in a 'new' expression returns a value. -->
      <!-- Result will be what the function returns, rather than a new instance of that  -->
      <!-- function.                                                                     -->
      <warn-constructor-returns-value>true</warn-constructor-returns-value>
      <!-- Enables reporting type coercions, which are unlikely to succeed. -->
      <warn-bad-type-cast>true</warn-bad-type-cast>
      <!-- Enables reporting calls to 'Boolean()' with no arguments.  They returned false -->
      <!-- in AS3, and they returned 'undefined' in AS2.                                  -->
      <warn-boolean-constructor-with-no-args>true</warn-boolean-constructor-with-no-args>
      <!-- Enables reporting 'null' arguments to Math functions.  In AS3, the arguments   -->
      <!-- are converted to '0'.  In AS2 they were was converted to 'NaN', and the result -->
      <!-- of the operation was often 'NaN'.                                              -->
      <warn-math-expressions-treat-null-like-zero>true</warn-math-expressions-treat-null-like-zero>
      <!-- Enables reporting of change in the way 'Array.toString()' handles 'null' and -->
      <!-- 'undefined' elements.  In FP7, it would print 'null' or 'undefined', it now  -->
      <!-- prints '' for both cases. -->
      <warn-array-tostring-changes>false</warn-array-tostring-changes>
      <!-- Enables reporting of no longer supported AS2 properties. -->
      <warn-deprecated-property-error>true</warn-deprecated-property-error>
      <!-- Enables reporting of no longer supported AS2 functions. -->
      <warn-deprecated-function-error>true</warn-deprecated-function-error>
      <!-- Enables reporting of 'super' usages outside of a Class method. -->
      <warn-super-only-works-for-class-objects>true</warn-super-only-works-for-class-objects>
      <!-- Enables reporting of member adding to a 'sealed' Class. -->
      <warn-class-is-sealed>true</warn-class-is-sealed>
      <!-- Enables reporting of changes in scoping for 'this'.  Class methods extracted  -->
      <!-- from an instance of a class will always resolve 'this' back to that instance. -->
      <!-- In AS2 'this' was looked up dynamically based on where the method is invoked  -->
      <!-- from.                                                                         -->
      <warn-scoping-change-in-this>false</warn-scoping-change-in-this>
      <!-- Enables reporting of missing namespace declarations. -->
      <warn-missing-namespace-decl>true</warn-missing-namespace-decl>
      <!-- Enables reporting of random iteration.  The order in which properties of an     -->
      <!-- object are iterated over within a '(for x in target)' statement is random in    -->
      <!-- AS3.  If you experience odd ordering behavior, inspect these loops to determine -->
      <!-- if this change in ordering may affect your code. -->
      <warn-for-var-in-changes>false</warn-for-var-in-changes>
      <!-- Enables reporting of when an event handler will not successfully be added as a -->
      <!-- listener.                                                                      -->
      <warn-deprecated-event-handler-error>true</warn-deprecated-event-handler-error>
      <!-- Enables reporting of missing constructors. -->
      <warn-no-constructor>true</warn-no-constructor>
      <!-- Enables reporting of Void parameters, which are no longer supported. -->
      <warn-function-parameter-declared-as-void>true</warn-function-parameter-declared-as-void>
      <!-- Enables reporting of when 'super()' is not called from a constructor. -->
      <warn-no-explicit-super-call-in-constructor>false</warn-no-explicit-super-call-in-constructor>
      <!-- Enables reporting of possible AS2 'XML' class usages. -->
      <warn-xml-class-has-changed>false</warn-xml-class-has-changed>
   </compiler>
   <!-- A list of runtime shared library URLs to be loaded before applications start. -->
   <!-- not set -->
   <!--
   <runtime-shared-libraries>
      <url>string</url>
      <url>string</url>
   </runtime-shared-libraries>
   -->
   <!-- Enables SWFs to access the network. -->
   <use-network>true</use-network>
   <!-- Metadata added to SWFs via the SWF Metadata tag. -->
   <metadata>
      <title>Adobe Flex 2.0b Application</title>
      <description>http://www.macromedia.com/flex</description>
      <publisher>unknown</publisher>
      <creator>unknown</creator>
      <language>EN</language>
   </metadata>
</flex-config>
